syntax = "proto3";

package com.macapayments.services.entity.${type.lowerCaseName};

option java_multiple_files = true;
option java_package = "com.macapayments.services.entity.${type.lowerCaseName}";
option java_outer_classname = "${type.name}Entity";

message $type.name {
#set ($index = 1)
  int32 id = $index;
#foreach( $field in $type.fields)
#set ($index = $index + 1)
  $field.ptype $field.tableFieldName = $index;
#end
#set ($index = $index + 1)
  int64 created_at = $index;
#set ($index = $index + 1)
  int64 updated_at = $index;
#set ($index = $index + 1)
  string created_by = $index;
#set ($index = $index + 1)
  string updated_by = $index;
#set ($index = $index + 1)
  bool is_deleted = $index;
}
